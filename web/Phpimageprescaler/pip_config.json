{
    "debug_mode": true,
    "retina": true,
    "jpg_quality": 100,
    "jpg_quality_retina": 80,
    "browser_cache": 604800,
    "cache_path": "cache/pip",
    "sharpen": false,
    "resolutions": {
        "mobile": 0,
        "medium": 1000,
        "large": 1300

    },
    "setup": {
        "article": {
            "dimensions": {
                "0": {
                    "w": 100
                },
                "1200": {
                    "w": 200
                }
            }
        },
        "flux-rss": {
            "dimensions": {
                "mobile": {
                    "w": 150
                },
                "large": {
                    "w": 80
                }
            },
            "filter": {
                "IMG_FILTER_GRAYSCALE": true,
                "IMG_FILTER_COLORIZE": [
                    100,
                    50,
                    0
                ],
                "IMG_FILTER_NEGATE": false,
                "IMG_FILTER_BRIGHTNESS": 0
            }
        }
    }
}