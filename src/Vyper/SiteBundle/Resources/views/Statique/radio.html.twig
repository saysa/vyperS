                            $("#currentCover").attr('src', 'http://japanfm.fr/cover/'+data[0].cover);
                        }

                        $("#currentArtist-player").html(data[0].artist);
                    }


                    $("#currentTitle").html(data[0].title);
                    $("#currentArtist").html(data[0].artist);

                    $("#get-title-1").html(data[1].title);
                    $("#get-artist-1").html(data[1].artist);
                    if (data[1].cover == '') {
                        $("#cover-1").attr('src', '{{ asset('assets/css/images/nocover_jfm.jpg') }}');
                    } else {
                        $("#cover-1").attr('src', 'http://japanfm.fr/cover/'+data[1].cover);
                    }


